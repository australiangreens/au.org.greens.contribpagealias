<?xml version="1.0"?>
<extension key="au.org.greens.contribpagealias" type="module">
  <file>contribpagealias</file>
  <name>Contribution Page Aliases</name>
  <description>This CiviCRM extension allows for the creation of aliases for contribution pages</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>John Twyman</author>
    <email>john.twyman@greens.org.au</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/australiangreens/au.org.greens.contribpagealias</url>
    <url desc="Documentation">https://github.com/australiangreens/au.org.greens.contribpagealias</url>
    <url desc="Support">https://github.com/australiangreens/au.org.greens.contribpagealias</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-02-14</releaseDate>
  <version>1.1.1</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.8</ver>
  </compatibility>
  <requires>
    <ext>nz.co.fuzion.entitysetting</ext>
  </requires>
  <comments>This module may have some bugs but is considered ready for broad use</comments>
  <civix>
    <namespace>CRM/Contribpagealias</namespace>
    <format>23.02.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
